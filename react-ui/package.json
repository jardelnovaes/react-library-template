{
  "name": "@jardelnovaes/react-ui",
  "version": "0.0.2-snapshot.44",
  "description": "Jardel Novaes library with UI components for React frontends",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint ./ --ext js,jsx,ts,tsx",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "rollup -c",
    "build-snapshot": "yarn build && npm version prerelease --no-git-tag-version --preid snapshot",
    "pack-dev": "rm -rf /tmp/react-ui-*.tgz && yarn build && npm pack --pack-destination /tmp",
    "link-lib": "yarn build && yarn link && cd node_modules/react && yarn link && cd ../react-dom && yarn link && cd ../@material-ui/core && yarn link && cd ../../",
    "unlink-lib": "yarn unlink && cd node_modules/react && yarn unlink && cd ../react-dom && yarn unlink && cd ../@material-ui/core && yarn unlink && yarn install --force",
    "link-app-to-lib": "yarn link @jardelnovaes/react-ui && yarn link react && yarn link react-dom && yarn link @material-ui/core",
    "unlink-app-to-lib": "yarn unlink @jardelnovaes/react-ui && yarn unlink react && yarn unlink react-dom && yarn unlink @material-ui/core && yarn install --force",
    "open-next-patch": "npm version prepatch --no-git-tag-version --preid snapshot",
    "open-next-version": "npm version preminor --no-git-tag-version --preid snapshot",
    "publish-snapshot": "yarn build-snapshot && cd dist && npm publish --tag snapshot --access public",
    "publish-release": "yarn build && cd dist && npm publish --access public"
  },
  "homepage": "https://github.com/jardelnovaes/react-library-template/blob/main/README.md",
  "author": "Jardel Novaes",
  "license": "Apache 2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jardelnovaes/react-library-template.git"
  },
  "publishConfig": {
    "registry": "http://??/repository/??/"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-react": "^7.16.7",
    "@fontsource/roboto": "^4.5.3",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@swc/jest": "^0.2.15",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.38",
