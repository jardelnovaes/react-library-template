    "eslint": "^7.24.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.29.4",
    "jest": "^27.4.5",
    "postcss": "^8.4.12",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.62.0",
